{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": []
  },
  "paths": {
    "@environment": ["./src/environments/environment.ts"],
    "@environment-interface": ["./src/environments/environment.type.ts"],
    "@/*": [
      "src/app/*"
    ]
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts"
  ]
}
